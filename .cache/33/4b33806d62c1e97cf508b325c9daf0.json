e":71,"column":9},"generated":{"line":1,"column":1405}},{"source":"src/index.js","name":"Transport","original":{"line":71,"column":9},"generated":{"line":1,"column":1407}},{"source":"src/index.js","name":"Transport","original":{"line":71,"column":9},"generated":{"line":1,"column":1415}},{"source":"src/index.js","name":"toggle","original":{"line":71,"column":19},"generated":{"line":1,"column":1425}},{"source":"src/index.js","name":"rectLifeSycles","original":{"line":72,"column":4},"generated":{"line":1,"column":1434}},{"source":"src/index.js","name":"push","original":{"line":72,"column":19},"generated":{"line":1,"column":1436}},{"source":"src/index.js","original":{"line":73,"column":6},"generated":{"line":1,"column":1441}},{"source":"src/index.js","name":"RectLifeSycle","original":{"line":73,"column":10},"generated":{"line":1,"column":1445}},{"source":"src/index.js","name":"p","original":{"line":73,"column":24},"generated":{"line":1,"column":1447}},{"source":"src/index.js","name":"mouseX","original":{"line":73,"column":26},"generated":{"line":1,"column":1449}},{"source":"src/index.js","name":"p","original":{"line":73,"column":34},"generated":{"line":1,"column":1456}},{"source":"src/index.js","name":"mouseY","original":{"line":73,"column":36},"generated":{"line":1,"column":1458}},{"source":"src/index.js","original":{"line":81,"column":0},"generated":{"line":1,"column":1469}},{"source":"src/index.js","name":"p5","original":{"line":81,"column":4},"generated":{"line":1,"column":1473}},{"source":"src/index.js","original":{"line":81,"column":0},"generated":{"line":1,"column":1475}},{"source":"src/index.js","name":"sketch","original":{"line":81,"column":7},"generated":{"line":1,"column":1483}},{"source":"src/index.js","name":"document","original":{"line":81,"column":14},"generated":{"line":1,"column":1485}},{"source":"src/index.js","name":"body","original":{"line":81,"column":23},"generated":{"line":1,"column":1494}}],"sources":{"src/index.js":"import p5 from 'p5'\nimport Tone from 'tone'\n\nconst sketch = p => {\n\n  let synth = new Tone.Synth().toMaster()\n  var part = new Tone.Part(\n    (time, event) => {\n      synth.triggerAttackRelease(event.note, event.dur, time)\n    }, \n    [\n      { time: 0,     note: 'E5', dur: '8n' },\n      { time: 0.1,   note: 'F5', dur: '8n' },\n    ]\n  )\n  \n  let waveform = new Tone.Analyser('waveform', 1024)\n  synth.connect(waveform)\n\n  class RectLifeSycle {\n\n    constructor(x, y) {\n      this.lifeSycle = []\n      for (let i=0; i<1; i+=0.01) {\n        this.lifeSycle.push(\n          _ => {\n            p.stroke(180+180*i, 100, 100)\n            p.rect(x, y, 300-300*i, 300-300*i, 10)\n          }\n        )\n      }\n    }\n\n    draw() {\n      if (this.lifeSycle.length == 0) return \n      this.lifeSycle.pop()()\n    }\n\n  }\n\n  let rectLifeSycles = []\n\n  p.setup = () => {\n    p.createCanvas(p.windowWidth, p.windowHeight)\n    p.background(0)\n    p.colorMode(p.HSB,360,100,100)\n    p.rectMode(p.CENTER)\n    part.start(0)\n  }\n\n  p.draw = () => {\n    // 背景を黒く塗る\n    p.background(0, 0, 0, 200)\n\n    // waveformを描く\n    if (!synth || !waveform) return;\n    waveform.getValue().forEach(\n      (elm, index) => {\n        if (index%8!=0) return\n        p.stroke(index/1024*200, 100, 100)\n        p.strokeWeight(20*elm)\n        p.point(index*(p.width/1024), p.height/2 + elm*p.height/2 )\n      }\n    )\n\n    p.noFill()\n    rectLifeSycles.forEach( elm => elm.draw() )\n  }\n\n  p.mouseReleased = () => {\n    Tone.Transport.toggle()\n    rectLifeSycles.push(\n      new RectLifeSycle(p.mouseX, p.mouseY)\n    )\n  }\n\n  \n\n}\n\nnew p5(sketch,document.body)"},"lineCount":null}},"error":null,"hash":"68f30308b5dd9ca3f17e9cfa89998481","cacheData":{"env":{}}}